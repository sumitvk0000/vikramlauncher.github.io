{
    
    "name": "Vikram launcher",
    "description": "Quick launch most useful media and save your time",
    "version": "1.0.0",
    "manifest_version":2,
    "icons":  { "128" : "icon_128.png" },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup" : "popup.html"
    },
    "permissions": ["activeTab"]
}